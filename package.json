{
  "name": "twitch-midi-bot",
  "version": "0.1.2",
  "description": "",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "build": "tsc",
    "lint": "eslint -c .eslintrc.json --ext .ts src/**/*.ts",
    "format": "eslint --fix -c .eslintrc.json --ext .ts src/{,**/}*.ts",
    "watch-build": "tsc -w",
    "serve": "npm run build && npm run start",
    "start": "node ./lib/index.js",
    "bundle": "ncc build src/index.ts -m -o dist -e jzz && pkg -o TwitchMidiBot -t node14-windows,node14-linux,node14-macos dist/index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@twurple/api": "^5.0.18",
    "@twurple/auth": "^5.0.18",
    "@twurple/chat": "^5.0.18",
    "@twurple/eventsub": "^5.0.18",
    "@twurple/pubsub": "^5.0.18",
    "@types/nanotimer": "^0.3.0",
    "dotenv": "^16.0.0",
    "harmonics": "^1.1.0",
    "nanotimer": "^0.3.15",
    "webmidi": "^3.0.15",
    "jzz": "^1.4.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vercel/ncc": "^0.33.3",
    "pkg": "^5.5.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint": "^8.11.0",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  },
  "prettier": {
    "printWidth": 180,
    "trailingComma": "none",
    "singleQuote": true,
    "tabWidth": 4
  },
  "bin": {
    "twitch-midi-bot": "lib/index.js"
  },
  "preferGlobal": true
}
